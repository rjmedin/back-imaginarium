{
  "version": 2,
  "routes": [
    {
      "src": "/api-docs(.*)",
      "dest": "/api/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/app.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "ENABLE_SWAGGER": "true"
  },
  "functions": {
    "api/app.js": {
      "runtime": "nodejs18.x"
    }
  }
}