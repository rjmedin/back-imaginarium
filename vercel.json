{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "ENABLE_SWAGGER": "true"
  },
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@3.2.0"
    }
  }
}